CPP_FLAGS = @BAACK_CPPFLAGS@

@BAACK_WITH_DYNSTACK_FALSE@@BAACK_WITH_REMOTECONTROL_FALSE@lib_LIBRARIES = -DBUILD_BASIC=off -DBUILD_DYNSTACK=off -DBUILD_REMOTECONTROL=off
@BAACK_WITH_DYNSTACK_FALSE@@BAACK_WITH_REMOTECONTROL_TRUE@lib_LIBRARIES =  -DBUILD_BASIC=on  -DBUILD_DYNSTACK=off -DBUILD_REMOTECONTROL=on
@BAACK_WITH_DYNSTACK_TRUE@@BAACK_WITH_REMOTECONTROL_FALSE@lib_LIBRARIES =  -DBUILD_BASIC=on  -DBUILD_DYNSTACK=on  -DBUILD_REMOTECONTROL=off
@BAACK_WITH_DYNSTACK_TRUE@@BAACK_WITH_REMOTECONTROL_TRUE@lib_LIBRARIES =   -DBUILD_BASIC=on  -DBUILD_DYNSTACK=on  -DBUILD_REMOTECONTROL=on

@BAACK_WITHTEST_TRUE@buildTest =  -DBUILD_TESTS=on
@BAACK_WITHTEST_FALSE@buildTest = -DBUILD_TESTS=off


all:
	@echo "Build All"
	cd $(CORSIKA_LIB_DIR) && cmake $(abs_builddir)  $(lib_LIBRARIES) $(buildTest) && make
	cd $(CORSIKA_LIB_DIR) && cp libBasic.a libBaack.a

install: all
	@echo "Build Install"

debug:
		#$(foreach v, $(.VARIABLES), $(info $(v) = $($(v))))

clean:

clean-recursive:


distclean:
	cd $(CORSIKA_LIB_DIR) && rm -rf *


.PHONY: clean distclean all install
